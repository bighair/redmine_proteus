<%= labelled_form_for :proteus, @proteus,
                             :url => {:action => 'update', :id => @proteus},
                             :html => {:id => 'proteus-form',
                                       :class => 'tabular proteus-form',
                                       :method => :put,
                                       :multipart => true} do |f| %>

    <%= render :partial => 'form', :locals => {:f => f} %>

  <%= submit_tag "Save Changes" %>
  <%= link_to 'Back', project_proteus_path %>
<% end %>
